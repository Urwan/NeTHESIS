	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "remuneration"@en , "empoyees."@en , "mzda"@cs , "salary"@en , "plat"@cs , "zaměstnanci."@cs , "wages"@en , "vzdělávání"@cs , "Odměňování"@cs , "education"@en ;
