	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "prozřetelnost"@cs , "cyclicality"@en , "ideální republika"@cs , "philosophy of history"@en , "three ages of nations."@en , "ideal republic"@en , "přirozenost národů"@cs , "cykličnost"@cs , "tři věky národů."@cs , "filosofie dějin"@cs , "Giambattista Vico"@en , "Giambattista Vico"@cs , "ideal history"@en , "nature of nations"@en , "ideální historie"@cs , "providence"@en ;
