	<http://schema.org/name> "Model of Evaluation of Economic Risk of Construction Project from the Perspective of Investor"@en , "Hodnocení ekonomického rizika investičního projektu z pohledu investora"@cs ;
	<http://schema.org/publisher> <http://nusl.netusil.cloud/organization/Vysok%C3%A9%20u%C4%8Den%C3%AD%20technick%C3%A9%20v%20Brn%C4%9B> , <http://nusl.netusil.cloud/organization/Vysok%C3%A9%20u%C4%8Den%C3%AD%20technick%C3%A9%20v%20Brn%C4%9B/Fakulta%20stavebn%C3%AD> ;
