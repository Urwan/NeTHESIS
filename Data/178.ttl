	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "milk efficiency"@en , "pastva"@cs , "pasture"@en , "cattle"@en , "skot"@cs , "mléčná užitkovost"@cs ;
	<http://schema.org/name> "Vyhodnocení vlivu pastvy na užitkovost dojnic"@cs , "Evaluation the influence of grazing on performance of dairy cows"@en ;
