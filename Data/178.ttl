	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "hermeneutics"@en , "Písmo svaté"@cs , "teologie"@cs , "biblické texty" , "methods of reading"@en , "Holy Scripture"@en , "linguistic"@en , "church"@en , "lingvistika"@cs , "Bible"@cs , "metody výkladu"@cs , "biblická hermeneutika"@cs , "adventist"@en , "exegesis"@en , "adventisté"@cs , "církev"@cs , "hermeneutika" , "hermeneutika"@cs , "exegeze"@cs , "biblical hermeneutics"@en , "theology"@en ;
