	<http://schema.org/name> "Composition of Public Expenditure and Growth: Is there a Nexus?"@en , "Composition of Public Expenditure and Growth: Is there a Nexus?"@cs ;
	<http://schema.org/publisher> <http://nusl.netusil.cloud/organization/Vysok%C3%A1%20%C5%A1kola%20ekonomick%C3%A1%20v%20Praze> ;
