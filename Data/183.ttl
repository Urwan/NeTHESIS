	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "Monte Carlo"@en , "Monte Carlo"@cs , "modely" , "DEA models"@en , "metoda Monte Carlo" , "interval data"@en , "intervalová data"@cs , "DEA modely"@cs ;
	<http://schema.org/name> "Aplikace metody Monte Carlo pro DEA modely s intervalovými charakteristikami"@cs , "Application of Monte Carlo method on DEA models with interval characteristics"@en ;
