	<http://schema.org/keywords> "Internet"@en , "chat"@en , "sociolingvistika"@en , "jazyk"@en , "lingvistika"@en ;
	<http://schema.org/name> "The Language of Internet Debates: A linguistic approach./Jazyk internetových debat: lingvistický pohled."@cs ;
