	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "demence"@cs , "care"@en , "Alzheimer?s diseae"@en , "péče"@cs , "family caregivers"@en , "rodinní pečující"@cs , "dementia"@en , "Alzheimerova choroba"@cs ;
	<http://schema.org/name> "Alzheimer's disease in the family"@en , "Alzheimerova choroba v rodině"@cs ;
