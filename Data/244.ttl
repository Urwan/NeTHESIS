	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "plodnost"@cs , "fertility"@en , "dojivost"@cs , "Automatic Milking System"@en , "milk yield"@en , "dojící roboty"@cs ;
	<http://schema.org/name> "PLODNOST KRAV CHOVANÝCH V TECHNOLOGIÍCH S AUTOMATIZOVANÝM DOJENÍM"@cs , "FERTILITY OF COWS KEPT IN MODERN TECHNOLOGY"@en ;
