	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "additive constant"@en , "metrology."@en , "Calibration"@en , "reflective prisms"@en , "elektronické dálkoměry"@cs , "Kalibrace"@cs , "součtová konstanta"@cs , "electronic distance meters"@en , "odrazné hranoly"@cs , "metrologie."@cs ;
