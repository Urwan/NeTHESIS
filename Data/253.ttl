	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "zvládání stresu"@cs , "stress"@en , "motivační struktura"@cs , "učitel mateřské školy"@cs , "resilience"@en , "resilience"@cs , "stres"@cs , "kindergarten teacher"@en , "motivace"@cs , "motivational structure"@en , "stress management"@en , "motivation"@en ;
	<http://schema.org/name> "Případová studie motivační struktury resilience učitelek MŠ"@cs , "The case study of motivational structure of resiliency of kindergarten teachers"@en ;
