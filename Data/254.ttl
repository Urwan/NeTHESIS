	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "interactive teaching"@en , "interaktivní výuka"@cs , "stereometry"@en , "stereometrie"@cs ;
	<http://schema.org/name> "Interactive stereometry education at a secondary technical school"@en , "Interaktivní výuka stereometrie na SOU"@cs ;
