	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "králík"@cs , "exteriér"@cs , "Czech Solver"@en , "rabbit"@en , "standard"@cs , "standard"@en , "genetic resources"@en , "exterior"@en , "genetické zdroje"@cs , "Český luštič"@cs ;
