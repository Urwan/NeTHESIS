	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "spisovatel"@cs , "epic"@en , "epika"@cs , "books"@en , "tvorba literárního příběhu"@cs , "tvůrčí psaní"@cs , "short story"@en , "povídka"@cs , "author"@en , "story"@en , "bakalářská práce."@cs , "literature"@en , "bachelor's thesis."@en , "literatura"@cs , "creation of a literary story"@en , "drama"@en , "drama"@cs , "writer"@en , "psaní"@cs , "knihy"@cs , "příběh"@cs , "writing"@en , "volný čas"@cs , "autor"@cs , "leisure time"@en , "Literární tvorba"@cs , "creative writing"@en ;
