	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "Yamamoto, Masao, 1957-"@cs , "teorie fotografie"@cs , "analýzy"@cs , "japonská fotografie"@cs , "fotografové"@cs ;
