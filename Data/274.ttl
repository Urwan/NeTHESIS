	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "analýzy filmových děl"@cs , "filmová teorie"@cs ;
	<http://schema.org/name> "The End of a Film or What to Do When Our Hero Is Still Alive"@en , "Koniec filmu, alebo čo robiť, keď nám to hrdina prežije"@cs ;
