	<http://schema.org/name> "Attitudes of Roma adolescents to the start of marriage and family"@en , "Postoje romských adolescentů k zakládání manželství a rodiny"@cs ;
	<http://schema.org/publisher> <http://nusl.netusil.cloud/organization/JIHO%C4%8CESK%C3%81%20UNIVERZITA%20V%20%C4%8CESK%C3%9DCH%20BUD%C4%9AJOVIC%C3%8DCH> ;
