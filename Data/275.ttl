	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "orientální tanec"@cs , "Creativity"@en , "audience"@en , "divák"@cs , "oriental dance"@en , "spectator"@en , "Kreativita"@cs , "publikum"@cs ;
	<http://schema.org/name> "Rozvoj kreativity při tvorbě vystoupení orientálního tance s ohledem na požadavky současného publika"@cs , "Development of creativity during the formation of the belly dance performance with regard to the requirements of the current audience"@en ;
