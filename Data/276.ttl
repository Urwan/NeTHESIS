	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "produktivita práce"@cs , "finanční analýza"@cs , "productivity of labour"@en , "financial analysis"@en ;
	<http://schema.org/name> "Productivity of manufacturing company"@en , "Produktivita výrobního podniku"@cs ;
