	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "mare"@en , "klisna"@cs , "stallion"@en , "overall rating"@en , "Zkoušky výkonnosti"@cs , "hřebec"@cs , "Performance tests"@en , "celkové hodnocení"@cs ;
	<http://schema.org/name> "Zkoušky výkonnosti chladnokrevných koní a jejich hodnocení"@cs , "Performance tests of cold-blooded horses and their evaluation"@en ;
