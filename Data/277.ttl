	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "pšenice"@cs , "malotechnologie"@cs , "glucose syrup"@en , "glukózový sirup"@cs , "wheat"@en , "malotechnology"@en , "farmářská výroba"@cs , "farm production"@en ;
	<http://schema.org/name> "Návrh farmářské výroby glukozového sirupu z pšenice"@cs , "Proposal for farm production of glucose syrup from wheat"@en ;
