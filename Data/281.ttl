	<http://schema.org/keywords> "taneční pedagogika"@cs , "tanec"@cs ;
	<http://schema.org/name> "Something between us - Work with a Group of Performers"@en , "Něco mezi námi - práce se skupinou interpretů"@cs ;
	<http://schema.org/publisher> <http://nusl.netusil.cloud/organization/Akademie%20m%C3%BAzick%C3%BDch%20um%C4%9Bn%C3%AD%20v%20Praze> , <http://nusl.netusil.cloud/organization/Akademie%20m%C3%BAzick%C3%BDch%20um%C4%9Bn%C3%AD%20v%20Praze/Hudebn%C3%AD%20a%20tane%C4%8Dn%C3%AD%20fakulta> ;
