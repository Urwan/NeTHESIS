	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "multiple-criteria decision-making"@en , "offers analysis"@en , "analýza nabídek"@cs , "vícekriteriální rozhodování"@cs , "optimal construction budget"@en , "optimální rozpočet stavby"@cs ;
	<http://schema.org/name> "Preparing a home construction budget"@en , "Rozpočet stavby domu"@cs ;
