	<http://schema.org/keywords> "přeprava materiálu"@cs , "úspory"@cs , "společnost ABB"@cs , "náklady"@cs , "elektronická aukce"@cs ;
	<http://schema.org/name> "Analýza a zhodnocení implementace elektronické aukce ve vybraném podniku"@cs ;
