<http://nusl.netusil.cloud/theses/222822> a <https://thesis.netusil.cloud/1.0/MastersThesis> , <http://schema.org/Thesis> ;
	<http://schema.org/author> <http://nusl.netusil.cloud/people/222822_0> ;
