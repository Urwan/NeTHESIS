	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "productionfactors"@en , "produktivita"@cs , "efficiency"@en , "výrobní faktory"@cs , "productivity"@en , "Efektivnost"@cs ;
	<http://schema.org/name> "Efektivnosti výrobních faktorů v podniku"@cs , "Efficiency of production factors in the company"@en ;
