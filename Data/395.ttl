	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "Predikční modely"@cs , "Logistic regression"@en , "Discriminant analysis"@en , "Diskriminační analýza"@cs , "Predictive models"@en , "Logistická regrese"@cs ;
	<http://schema.org/name> "Building predictive models"@en , "Tvorba predikčních modelů"@cs ;
