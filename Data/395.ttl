	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "landscape"@en , "rozptýlená zeleň"@cs , "scattered vegetation functions"@en , "funkce rozptýlené zeleně"@cs , "krajina"@cs , "scatterd vegetation"@en ;
