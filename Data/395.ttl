	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "mikroklima stáje"@cs , "milk yield"@en , "plodnost"@cs , "mléčná užitkovost"@cs , "temperature-humidity index"@en , "microclimate of stables"@en , "fertility"@en , "teplotně vlhkostní index"@cs ;
