	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "competitiveness"@en , "konkurenceschopnost"@cs , "vědecko-technický park"@cs , "science and technology park"@en , "inovace"@cs , "Evropa"@cs , "Europe"@en , "vědeko-technologický rozvoj"@cs , "science and technology development"@en , "innovation"@en ;
	<http://schema.org/name> "Integration Tendencies of the Science and Technology Development in Europe: The Competitiveness' Growth"@en , "Integrační tendence vědecko-technologického rozvoje v Evropě: růst konkurenceschopnosti"@cs ;
