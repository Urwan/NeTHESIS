	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "Finanční krize"@cs , "Irsko"@cs , "hospodářský cyklus"@cs , "Financial Crisis"@en , "Business Cycle"@en , "Ireland"@en ;
