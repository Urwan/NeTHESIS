	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "Praxeology"@en , "Teleology"@en , "Apriorism"@en , "Apriorismus"@cs , "Praxeologie"@cs , "Teleologie"@cs ;
