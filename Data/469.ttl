	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "Turkey"@en , "Islam"@en , "Turecko"@cs , "human rights"@en , "Pakistan"@en , "Pákistán"@cs , "islámské právo šarí´a"@cs , "lidská práva"@cs , "islám"@cs , "Islamic law Sharia"@en , "international relations"@en , "mezinárodní vztahy"@cs ;
	<http://schema.org/name> "Pojetí lidských práv v islámu a jeho odraz v mezinárodních vztazích."@cs , "The conception of human rights in Islam and its impact on international relations"@en ;
