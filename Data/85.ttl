	<http://schema.org/inLanguage> "cze" ;
	<http://schema.org/keywords> "dotace"@cs , "zemědělství"@cs , "subsidy"@en , "region"@en , "region"@cs , "agriculture"@en ;
	<http://schema.org/name> "The Impact of Subsidy on the Development of Agriculture in the South Bohemian Region"@en , "Dopad dotačních příjmů na rozvoj zemědělství v Jihočeském kraji"@cs ;
